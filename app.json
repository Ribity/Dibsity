{
  "expo": {
    "name": "Kibity",
    "slug": "Kibity",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "scheme": "kibity",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#008000",
    "icon": "./assets/images/PurpleFaceIcon1024.png",
    "splash": {
      "image": "./assets/images/PurpleFaceSplash1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#D4AF37"
    },
    "updates": {
      "fallbackToCacheTimeout": 30000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kibity.kibity",
      "icon": "./assets/images/PurpleFaceIcon1024.png"
    },
    "android": {
      "package": "com.kibity.kibity",
      "versionCode": 1,
      "icon": "./assets/images/purpleFaceIcon512.png",
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.kibity.kibity"
    },
    "androidStatusBarColor": "#000000",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    },
  "description": "Personalized children's stories"
  }
}
